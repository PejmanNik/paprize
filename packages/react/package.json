{
    "name": "@paprize/react",
    "version": "0.0.11",
    "description": "React library for creating professional, print-ready reports",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "./dist/paprize-react.umd.cjs",
    "module": "./dist/paprize-react.js",
    "exports": {
        ".": {
            "types": "./dist/paprize-react.d.ts",
            "import": "./dist/paprize-react.js",
            "require": "./dist/paprize-core.umd.cjs"
        }
    },
    "keywords": [
        "paper",
        "print",
        "report",
        "pdf",
        "html2pdf",
        "react"
    ],
    "homepage": "https://github.com/PejmanNik/paprize#readme",
    "bugs": {
        "url": "https://github.com/PejmanNik/paprize/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PejmanNik/paprize.git"
    },
    "license": "Apache-2.0",
    "author": "PejmanNikram",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "prepack": "node ../../utils/scripts/prepack.mjs"
    },
    "dependencies": {
        "@paprize/core": "workspace:*"
    },
    "peerDependencies": {
        "react": ">=17.0.0",
        "react-dom": ">=17.0.0"
    },
    "devDependencies": {
        "@paprize/config": "workspace:*",
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "@vitejs/plugin-react-swc": "^4.1.0",
        "react": "catalog:",
        "react-dom": "catalog:",
        "typescript": "catalog:",
        "vite": "catalog:"
    }
}